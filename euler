#include <cstdio>
#include <vector>
#include <iostream>
#include <list>

using namespace std;

int graph[1000][1000]; 
int n; 
int i, j, degree[1000] = {},f=1;
list<int> lt1;

void oiler(int u) {
        for(int v=0;v<n;v++)
        {
            while (graph[u][v]>0) 
            {
                graph[u][v]--;
                graph[v][u]--;
                oiler(v);
                }
        }
        lt1.push_back(u);
 }
 
int main(void) 
{
    cin >> n;
    for (i = 0; i < n; i++) 
    { 
        for (j = 0; j < n; j++) 
        {
            cin >> graph[i][j];
            sum += graph[i][j]; 
        }
    }
    oiler(0);
    lt1.reverse();
    list<int>::iterator iter = lt1.begin();
    for (auto const& item : lt1)
    {
    std::cout << item;
    }
}
